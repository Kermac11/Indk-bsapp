@page
@model Indkøbsapp.Pages.Brugere.BrugerEditModel
@{
}
<form method="post">
    <input type="hidden" asp-for="Bruger.CreationDate" />
    <input type="hidden" asp-for="Bruger.UserName" />
    <div class="form-group">
        <label asp-for="@Model.Bruger.Navn" class="custom-control-label"></label>
        <input asp-for="@Model.Bruger.Navn" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.Bruger.PassWord" class="custom-control-label"></label>
        <input type="password" asp-for="@Model.Bruger.PassWord" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.Bruger.Adresse" class="custom-control-label"></label>
        <input asp-for="@Model.Bruger.Adresse" class="form-control" />
    </div>
    <div class="col-md-4">
        <div class="form-group">
            <label asp-for="@Model.Bruger.Leverandør" class="custom-control-label"></label>
            <input type="checkbox" asp-for="@Model.Bruger.Leverandør" class="form-control" />
        </div>
    </div>
    <input type="submit" value="create" class="btn btn-primary" />
</form>